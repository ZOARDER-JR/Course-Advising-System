<Window x:Class="CourseAdvising.DeleteStudent"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Delete Student" Height="200" Width="300">
    <Grid Background="Aquamarine">
        <Label Height="30" HorizontalAlignment="Left" Margin="12,33,0,0" Name="label1" VerticalAlignment="Top" Width="91">Student ID</Label>
        <TextBox Height="23" Margin="120,33,12,0" Name="studentID" VerticalAlignment="Top" />
        <Button Height="35" HorizontalAlignment="Left" Margin="12,0,0,25" Name="delete" VerticalAlignment="Bottom" Width="95" Click="delete_Click">Delete</Button>
        <Button Height="35" HorizontalAlignment="Right" Margin="0,0,12,25" Name="cancel" VerticalAlignment="Bottom" Width="95" Click="cancel_Click">Cancel</Button>
    </Grid>
</Window>
