<Window x:Class="CourseAdvising.DeleteInstructor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="DeleteInstructor" Height="200" Width="300" Background="Aquamarine">
    <Grid>
        <Label Height="30" HorizontalAlignment="Left" Margin="12,30,0,0" Name="label1" VerticalAlignment="Top" Width="91">Instructor ID</Label>
        <TextBox Height="23" Margin="120,30,12,0" Name="instructorID" VerticalAlignment="Top" />
        <Button HorizontalAlignment="Left" Margin="12,0,0,28" Name="delete" Width="95" Height="35" VerticalAlignment="Bottom" Click="delete_Click">Delete</Button>
        <Button HorizontalAlignment="Right" Margin="0,0,12,28" Name="cancel" Width="95" Height="35" VerticalAlignment="Bottom" Click="cancel_Click">Cancel</Button>
    </Grid>
</Window>
