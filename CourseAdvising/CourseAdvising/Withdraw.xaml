<Window x:Class="CourseAdvising.Withdraw"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Withdraw" Height="300" Width="400" Background="Aquamarine" Loaded="Window_Loaded">
    <Grid>
        <Label Height="28" HorizontalAlignment="Left" Margin="30,44,0,0" Name="label1" VerticalAlignment="Top" Width="120">Student ID</Label>
        <Label Height="28" HorizontalAlignment="Left" Margin="30,99,0,0" Name="label2" VerticalAlignment="Top" Width="120">Course ID</Label>
        <TextBox Height="26" Margin="0,44,15,0" Name="StudentID" VerticalAlignment="Top" HorizontalAlignment="Right" Width="169" />
        <TextBox Height="26" Margin="0,101,15,0" Name="CourseID" VerticalAlignment="Top" HorizontalAlignment="Right" Width="169" />
        <Button Height="23" HorizontalAlignment="Left" Margin="30,0,0,40" Name="withdraw" VerticalAlignment="Bottom" Width="75" Click="withdraw_Click">Withdraw</Button>
        <Button Height="23" HorizontalAlignment="Right" Margin="0,0,44,40" Name="Cancel" VerticalAlignment="Bottom" Width="75" Click="Cancel_Click">Cancel</Button>
    </Grid>
</Window>
